{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "never"],
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-console":"off",
        "linebreak-style": ["error", "windows"]
    }
}
